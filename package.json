{
    "name": "open-src-form-injector",
    "version": "1.0.0",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "react-devtools": "react-devtools",
        "start": "node -e \"const { website } = require('./imgnx.json'); require('child_process').execSync(`WEBSITE=${website} webpack-dev-server --mode production --port 8000`)\"",
        "dev": "node -e \"const { website } = require('./imgnx.json'); require('child_process').execSync(`WEBSITE=${website} webpack-dev-server --mode development --port 3000`)\"",
        "serve": "node -e \"webpack serve --config webpack.config.mjs --mode production\"",
        "serve:dev": "node -e \"webpack serve --config webpack.config.mjs --mode development\"",
        "watch": "node -e \"webpack --mode production --watch\"",
        "watch:dev": "node -e \"webpack --mode development --watch\"",
        "build": "npx webpack --mode production",
        "build:dev": "node -e \"webpack --mode development\"",
        "fetch": "cp -r @/0Scrape/dist/** ./src/ && tailwindcss -i ./src/**/**.css -o ./dist/output.css --minify"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@adobe/react-spectrum": "^3.37.1",
        "@babel/core": "^7.26.0",
        "@babel/preset-react": "^7.25.9",
        "@babel/runtime": "^7.26.0",
        "@spectrum-icons/workflow": "^4.2.15",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.9",
        "@tailwindcss/typography": "^0.5.15",
        "@webpack-cli/serve": "^2.0.5",
        "autoprefixer": "^10.4.20",
        "babel-loader": "^9.2.1",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.2",
        "formik": "^2.4.6",
        "html-webpack-plugin": "^5.6.3",
        "postcss": "^8.4.47",
        "postcss-loader": "^8.1.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "style-loader": "^4.0.0",
        "tailwindcss": "^3.4.14",
        "ts-loader": "^9.5.1",
        "typescript": "^5.6.3",
        "webpack": "^5.96.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0",
        "yargs": "^17.7.2"
    },
    "packageManager": "yarn@4.5.1",
    "devDependencies": {
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/readline-sync": "^1",
        "chalk": "^5.3.0",
        "react-devtools": "^6.0.1",
        "readline-sync": "^1.4.10"
    }
}
